{
  "AppSettings": {
    "Environment": "Development",
    "ProcessingMode": "Real",
    "StorageMode": "Database",
    "EnableAuth": false,
    "EnableWebSockets": true,
    "EnableMetrics": true,
    "EnableRealProcessing": true,
    "EnableDocs": true,
    "EnableCors": true
  },
  "OpenAI": {
    "ApiKey": "your-openai-api-key-here"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=3306;Database=youtube_rag_real;Uid=youtube_rag_user;Pwd=youtube_rag_password;",
    "Redis": "localhost:6379",
    "VectorDatabase": "Host=localhost;Database=youtube_rag_vectors;Username=postgres;Password=postgres;"
  },
  "JwtSettings": {
    "SecretKey": "development-secret-key-please-change-in-production-youtube-rag-api-2024",
    "ExpiryMinutes": 1440,
    "RefreshTokenExpiryDays": 30
  },
  "AllowedOrigins": [
    "http://localhost:3000",
    "http://localhost:3001",
    "http://localhost:8000",
    "http://localhost:8080",
    "http://127.0.0.1:3000",
    "http://127.0.0.1:8000"
  ],
  "RateLimiting": {
    "PermitLimit": 50,
    "WindowMinutes": 1
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information",
      "YoutubeRag": "Debug"
    }
  }
}