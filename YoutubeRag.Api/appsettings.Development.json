{
  "AppSettings": {
    "Environment": "Development",
    "ProcessingMode": "Mock",
    "StorageMode": "Database",
    "EnableAuth": false,
    "EnableWebSockets": true,
    "EnableMetrics": false,
    "EnableRealProcessing": false,
    "EnableDocs": true,
    "EnableCors": true
  },
  "RateLimiting": {
    "PermitLimit": 200,
    "WindowMinutes": 1
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  }
}