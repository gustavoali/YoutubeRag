{
  "AppSettings": {
    "Environment": "Production",
    "ProcessingMode": "Real",
    "StorageMode": "Database",
    "EnableAuth": true,
    "EnableWebSockets": true,
    "EnableMetrics": true,
    "EnableRealProcessing": true,
    "EnableDocs": false,
    "EnableCors": true
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowMinutes": 1
  },
  "AllowedOrigins": [
    "https://yourdomain.com",
    "https://www.yourdomain.com"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  }
}